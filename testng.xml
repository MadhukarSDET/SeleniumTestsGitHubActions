<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="6">
	<test name="Edge Test" parallel="methods" thread-count="2">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="tests.ContactUSPageTests" />
			<class name="tests.GoogleSearchTest"></class>
		</classes>
	</test> <!-- Test -->
	<test name="Chrome Test" parallel="methods" thread-count="2">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="tests.ContactUSPageTests" />
			<class name="tests.GoogleSearchTest"></class>
		</classes>
	</test> <!-- Test -->
	<test name="Firefox Test" parallel="methods" thread-count="2">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="tests.ContactUSPageTests" />
			<class name="tests.GoogleSearchTest"></class>
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->